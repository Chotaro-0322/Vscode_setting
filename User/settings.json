{
    "workbench.colorTheme": "Default Dark+",
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
    "vscode-neovim.neovimInitVimPaths.linux": "/home/itolab-chotaro/.config/nvim/init.vim",
    "vscode-neovim.neovimExecutablePaths.win32": "/home/itolab-chotaro/.config/nvim/init.vim",
    "vscode-neovim.neovimInitVimPaths.win32": "/home/itolab-chotaro/.config/nvim/init.vim",
    "vscode-neovim.neovimInitVimPaths.darwin": "/home/itolab-chotaro/.config/nvim/init.vim",
    "vim.insertModeKeyBindings": [
        {
        "before": ["f", "d"],
        "after": ["<Esc>"]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
        "before": ["<space>", "f", "s"],
        "commands": ["workbench.action.files.save"]
        },
        { 
        "before": ["<space>", "w", "/"],
        "commands": ["workbench.action.splitEditor"]
        }
        { 
        "before": ["<space>", "w", "-"],
        "commands": ["workbench.action.splitEditorOrthogonal"]
        }
        { 
        "before": ["<space>", "w", "d"],
        "commands": ["workbench.action.closeActiveEditor"]
        }
    ],
    "vim.hlsearch": true
}